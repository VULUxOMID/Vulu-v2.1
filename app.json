{
  "expo": {
    "name": "VULU",
    "slug": "vulugonew",
    "version": "0.0.3",
    "orientation": "portrait",
    "userInterfaceStyle": "dark",
    "splash": {
      "image": "./assets/icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#1C1D23"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.vulugonew.app",
      "usesAppleSignIn": true,
      "infoPlist": {
        "NSCameraUsageDescription": "VuluGO needs access to your camera to enable live streaming.",
        "NSMicrophoneUsageDescription": "VuluGO needs access to your microphone to enable live streaming audio."
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#1C1D23"
      },
      "package": "com.vulugonew.app",
      "permissions": [
        "android.permission.CAMERA",
        "android.permission.RECORD_AUDIO",
        "android.permission.USE_BIOMETRIC",
        "android.permission.USE_FINGERPRINT"
      ]
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-router",
      "expo-apple-authentication",
      "@react-native-google-signin/google-signin",
      "expo-local-authentication",
      "expo-secure-store",
      [
        "expo-camera",
        {
          "cameraPermission": "Allow VuluGO to access your camera to enable live streaming.",
          "microphonePermission": "Allow VuluGO to access your microphone to enable live streaming audio.",
          "recordAudioAndroid": true
        }
      ]
    ],
    "scheme": "vulugonew",
    "experiments": {
      "tsconfigPaths": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "1605b9e3-65c9-49fc-bbc5-1cfece90dffd"
      }
    }
  }
}
