# VuluGO Staging Environment Configuration
# This file contains staging-specific environment variables

# Environment
NODE_ENV=staging
EXPO_PUBLIC_ENVIRONMENT=staging

# API Configuration
EXPO_PUBLIC_API_URL=https://staging-api.vulugo.com
EXPO_PUBLIC_API_VERSION=v1

# Firebase Configuration (Staging)
EXPO_PUBLIC_FIREBASE_API_KEY=your-staging-firebase-api-key
EXPO_PUBLIC_FIREBASE_AUTH_DOMAIN=vulugo-staging.firebaseapp.com
EXPO_PUBLIC_FIREBASE_PROJECT_ID=vulugo-staging
EXPO_PUBLIC_FIREBASE_STORAGE_BUCKET=vulugo-staging.appspot.com
EXPO_PUBLIC_FIREBASE_MESSAGING_SENDER_ID=your-staging-sender-id
EXPO_PUBLIC_FIREBASE_APP_ID=your-staging-app-id
EXPO_PUBLIC_FIREBASE_MEASUREMENT_ID=your-staging-measurement-id

# Agora Configuration (Staging)
EXPO_PUBLIC_AGORA_APP_ID=your-staging-agora-app-id
EXPO_PUBLIC_AGORA_APP_CERTIFICATE=your-staging-agora-certificate
EXPO_PUBLIC_AGORA_CUSTOMER_ID=your-staging-customer-id
EXPO_PUBLIC_AGORA_CUSTOMER_SECRET=your-staging-customer-secret

# Streaming Configuration
EXPO_PUBLIC_ENABLE_VIDEO_STREAMING=true
EXPO_PUBLIC_DEFAULT_STREAM_PROFILE=HD
EXPO_PUBLIC_MAX_PARTICIPANTS_PER_STREAM=25

# Twilio Configuration (Staging)
EXPO_PUBLIC_TWILIO_ACCOUNT_SID=your-staging-twilio-sid
EXPO_PUBLIC_TWILIO_API_KEY_SID=your-staging-twilio-key-sid
EXPO_PUBLIC_TWILIO_API_KEY_SECRET=your-staging-twilio-key-secret
EXPO_PUBLIC_TWILIO_FROM_NUMBER=your-staging-twilio-number

# SMS Configuration
EXPO_PUBLIC_SMS_SERVICE=twilio
EXPO_PUBLIC_USE_MOCK_SMS=false

# Google Sign-In (Staging)
EXPO_PUBLIC_GOOGLE_WEB_CLIENT_ID=your-staging-google-web-client-id
EXPO_PUBLIC_GOOGLE_IOS_CLIENT_ID=your-staging-google-ios-client-id

# Feature Flags
EXPO_PUBLIC_ENABLE_DEBUG_MODE=true
EXPO_PUBLIC_ENABLE_MOCK_SERVICES=false
EXPO_PUBLIC_ENABLE_TESTING_TOOLS=true
EXPO_PUBLIC_ENABLE_VERBOSE_LOGGING=false

# Security Configuration
EXPO_PUBLIC_ENABLE_RATE_LIMITING=true
EXPO_PUBLIC_ENABLE_DDOS_PROTECTION=true
EXPO_PUBLIC_ENABLE_CONTENT_FILTERING=true
EXPO_PUBLIC_ENABLE_SECURITY_AUDIT=true

# Monitoring Configuration
EXPO_PUBLIC_ENABLE_CRASHLYTICS=true
EXPO_PUBLIC_ENABLE_ANALYTICS=true
EXPO_PUBLIC_ENABLE_PERFORMANCE_MONITORING=true
EXPO_PUBLIC_LOG_LEVEL=info
EXPO_PUBLIC_ENABLE_REMOTE_LOGGING=true

# Performance Configuration
EXPO_PUBLIC_ENABLE_CODE_SPLITTING=true
EXPO_PUBLIC_ENABLE_IMAGE_OPTIMIZATION=true
EXPO_PUBLIC_ENABLE_CACHING=true
EXPO_PUBLIC_CACHE_TIMEOUT=3600000
EXPO_PUBLIC_ENABLE_COMPRESSION=true

# Development Tools
EXPO_PUBLIC_ENABLE_FLIPPER=true
EXPO_PUBLIC_ENABLE_REACTOTRON=true

# App Store Configuration
EXPO_PUBLIC_APP_STORE_URL=https://apps.apple.com/app/vulugo-staging
EXPO_PUBLIC_PLAY_STORE_URL=https://play.google.com/store/apps/details?id=com.vulugo.staging

# Support Configuration
EXPO_PUBLIC_SUPPORT_EMAIL=support-staging@vulugo.com
EXPO_PUBLIC_SUPPORT_URL=https://staging-support.vulugo.com

# Legal Configuration
EXPO_PUBLIC_PRIVACY_POLICY_URL=https://staging.vulugo.com/privacy
EXPO_PUBLIC_TERMS_OF_SERVICE_URL=https://staging.vulugo.com/terms

# Social Media
EXPO_PUBLIC_TWITTER_URL=https://twitter.com/vulugo_staging
EXPO_PUBLIC_INSTAGRAM_URL=https://instagram.com/vulugo_staging
EXPO_PUBLIC_DISCORD_URL=https://discord.gg/vulugo-staging

# CDN Configuration
EXPO_PUBLIC_CDN_URL=https://staging-cdn.vulugo.com
EXPO_PUBLIC_ASSETS_URL=https://staging-assets.vulugo.com

# WebSocket Configuration
EXPO_PUBLIC_WS_URL=wss://staging-ws.vulugo.com
EXPO_PUBLIC_WS_RECONNECT_INTERVAL=5000
EXPO_PUBLIC_WS_MAX_RECONNECT_ATTEMPTS=10

# Push Notifications
EXPO_PUBLIC_PUSH_NOTIFICATIONS_ENABLED=true
EXPO_PUBLIC_FCM_SENDER_ID=your-staging-fcm-sender-id

# Deep Linking
EXPO_PUBLIC_DEEP_LINK_SCHEME=vulugo-staging
EXPO_PUBLIC_UNIVERSAL_LINK_DOMAIN=staging.vulugo.com

# Sentry Configuration (Error Tracking)
EXPO_PUBLIC_SENTRY_DSN=your-staging-sentry-dsn
EXPO_PUBLIC_SENTRY_ENVIRONMENT=staging

# Analytics
EXPO_PUBLIC_MIXPANEL_TOKEN=your-staging-mixpanel-token
EXPO_PUBLIC_AMPLITUDE_API_KEY=your-staging-amplitude-key

# A/B Testing
EXPO_PUBLIC_OPTIMIZELY_SDK_KEY=your-staging-optimizely-key

# Content Delivery
EXPO_PUBLIC_CLOUDINARY_CLOUD_NAME=vulugo-staging
EXPO_PUBLIC_CLOUDINARY_API_KEY=your-staging-cloudinary-key

# Payment Processing (Test Mode)
EXPO_PUBLIC_STRIPE_PUBLISHABLE_KEY=pk_test_your-staging-stripe-key
EXPO_PUBLIC_PAYPAL_CLIENT_ID=your-staging-paypal-client-id

# Geolocation Services
EXPO_PUBLIC_GOOGLE_MAPS_API_KEY=your-staging-google-maps-key
EXPO_PUBLIC_MAPBOX_ACCESS_TOKEN=your-staging-mapbox-token

# Machine Learning / AI
EXPO_PUBLIC_OPENAI_API_KEY=your-staging-openai-key
EXPO_PUBLIC_AZURE_COGNITIVE_KEY=your-staging-azure-key

# Database Configuration
EXPO_PUBLIC_DATABASE_URL=postgresql://staging-db.vulugo.com:5432/vulugo_staging
EXPO_PUBLIC_REDIS_URL=redis://staging-redis.vulugo.com:6379

# Email Service
EXPO_PUBLIC_SENDGRID_API_KEY=your-staging-sendgrid-key
EXPO_PUBLIC_MAILGUN_API_KEY=your-staging-mailgun-key

# File Storage
EXPO_PUBLIC_AWS_S3_BUCKET=vulugo-staging-uploads
EXPO_PUBLIC_AWS_REGION=us-east-1

# Rate Limiting
EXPO_PUBLIC_RATE_LIMIT_WINDOW=900000
EXPO_PUBLIC_RATE_LIMIT_MAX_REQUESTS=100

# Session Configuration
EXPO_PUBLIC_SESSION_TIMEOUT=3600000
EXPO_PUBLIC_REFRESH_TOKEN_EXPIRY=604800000

# Maintenance Mode
EXPO_PUBLIC_MAINTENANCE_MODE=false
EXPO_PUBLIC_MAINTENANCE_MESSAGE="VuluGO is currently under maintenance. Please try again later."

# Version Information
EXPO_PUBLIC_APP_VERSION=1.0.0-staging
EXPO_PUBLIC_BUILD_NUMBER=1
EXPO_PUBLIC_MIN_SUPPORTED_VERSION=1.0.0
