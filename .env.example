# Google OAuth Configuration
# Get these from Google Cloud Console: https://console.cloud.google.com/
EXPO_PUBLIC_GOOGLE_WEB_CLIENT_ID=your-web-client-id.apps.googleusercontent.com
EXPO_PUBLIC_GOOGLE_IOS_CLIENT_ID=your-ios-client-id.apps.googleusercontent.com

# Firebase Configuration (if needed for additional services)
# EXPO_PUBLIC_FIREBASE_API_KEY=your-firebase-api-key
# EXPO_PUBLIC_FIREBASE_AUTH_DOMAIN=your-project.firebaseapp.com
# EXPO_PUBLIC_FIREBASE_PROJECT_ID=your-project-id

# Twilio SMS Configuration
EXPO_PUBLIC_TWILIO_ACCOUNT_SID=your-twilio-account-sid
EXPO_PUBLIC_TWILIO_AUTH_TOKEN=your-twilio-auth-token
EXPO_PUBLIC_TWILIO_FROM_NUMBER=your-twilio-phone-number

# Agora Live Streaming Configuration
# Get these from Agora Console: https://console.agora.io/
EXPO_PUBLIC_AGORA_APP_ID=your-agora-app-id
EXPO_PUBLIC_AGORA_APP_CERTIFICATE=your-agora-app-certificate
EXPO_PUBLIC_AGORA_CUSTOMER_ID=your-agora-customer-id
EXPO_PUBLIC_AGORA_CUSTOMER_SECRET=your-agora-customer-secret

# Streaming Configuration
EXPO_PUBLIC_ENABLE_VIDEO_STREAMING=true
EXPO_PUBLIC_DEFAULT_STREAM_PROFILE=AUDIO_ONLY
EXPO_PUBLIC_MAX_PARTICIPANTS_PER_STREAM=50

# Security Configuration (optional)
# EXPO_PUBLIC_ENABLE_SECURITY_LOGGING=true
# EXPO_PUBLIC_MAX_LOGIN_ATTEMPTS=5
# EXPO_PUBLIC_ACCOUNT_LOCK_DURATION_MINUTES=30
